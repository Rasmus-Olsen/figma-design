---

import BaseLayout from "../layouts/BaseLayout.astro";

import HeroPrimary from "../components/HeroPrimary.astro";

import ImageText from "../components/ImageText.astro";

import ScrollingContainer from "../components/ScrollingContainer.astro";
const projections = await fetch("https://raw.githubusercontent.com/Rasmus-Olsen/frontend-design/main/resources/json/projections.json").then((res) => res.json());

import CoreValues from "../components/CoreValues.astro";
const coreValues = await fetch("https://raw.githubusercontent.com/Rasmus-Olsen/frontend-design/main/resources/json/corevalues.json").then((res) => res.json());


---


<BaseLayout title="Home">
  <!-- <EmployeeSection data={employeeData}/> -->
  <!-- <EmployeeSection data={FirstThreeEmployess}/> -->
  
  <!-- <AccordionContainer data={faqData} exclusive="true" /> -->

  <HeroPrimary/>
  <ImageText side="left" tag="WE'LL WORK WITH YOU" heading="What To Expect" text="Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat." listOne="Bring to the table win-win survival strategies to ensure proactive domination. At the end of the day." listTwo="Capitalize on low hanging fruit to identify a ballpark value added activity to beta test override the digital." imgUrl="../src/assets/images/image-text1.png"/>
  
  <ScrollingContainer projections={projections} />
  <CoreValues coreValues={coreValues} surface="primary" tag="Growing with our Client" heading="Our core values & principles" button="Get in touch" description="Capitalize on low hanging fruit to identify a ballpark value added activity beta test. Override the digital divide with additional from DevOps."/>

</BaseLayout>



